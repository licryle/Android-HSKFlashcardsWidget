        {{Definition}}
        {{tts en:Definition}}<br /><br />
        {{Notes}}
        <br /><br />

        <hr id="example" />

        <div class="metadata">
            <div>{{HSK}} - {{Level}}</div>
        </div>
    ]]></string>

