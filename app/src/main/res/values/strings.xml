    <string name="anki_card_CNEN_front"><![CDATA[
        <h1 class="mobile">{{Hanzi}}</h1>

        <br />

        {{tts zh:Hanzi}}
        <div class="metadata">
            <div>{{HSK}} - {{Level}}</div>
        </div>
    ]]></string>

    <string name="anki_card_CNEN_back"><![CDATA[
        <h1 class="mobile">{{Hanzi}}</h1>

        {{Pinyin}}<br />

        {{tts zh:Hanzi}}

        <hr id="answer" />
        {{Definition}}
        {{tts en:Definition}}<br /><br />
        {{Notes}}
        <br /><br />

        <hr id="example" />

        <div class="metadata">
            <div>{{HSK}} - {{Level}}</div>
        </div>
    ]]></string>

